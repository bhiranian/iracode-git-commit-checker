{
  "name": "iracode/git-commit-checker",
  "description": "Check coding standard & code syntax with Git pre-commit hook.",
  "type": "package",
  "license": "MIT",
  "keywords": [
    "iracode",
    "iracode cms",
    "iracode platform",
    "iracode git commit checker"
  ],
  "homepage": "https://iracode.com",
  "authors": [
    {
      "name": "Sang Nguyen",
      "email": "sangnguyenplus@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Iracode\\GitCommitChecker\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Iracode\\GitCommitChecker\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^7.1.3|^8.0|^8.1",
    "laravel/framework": "^5.6|^6|^7|^8|^9",
    "php-parallel-lint/php-parallel-lint": "^1.0",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1|^7.0|^8.0|^9.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Iracode\\GitCommitChecker\\Providers\\GitCommitCheckerServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable"
}
